#include<iostream>
using namespace std;
int main(){{
    int member;
    int beli;
    cout<<"member:";
    cin>>member;
    cout<<"beli:";
    cin>>beli;
    if(member==1){if(beli>=1000000){cout<<"selamat anda mendapatkan diskon rp 100.000,00 sehingga membayar rp "<<beli-100000;}else cout<<"selamat anda mendapatkan diskon rp 90.000,00 sehingga membayar rp "<<beli-90000;}
    else if(member==2){if(beli>=1000000){cout<<"selamat anda mendapatkan diskon rp 80.000,00 sehingga membayar rp "<<beli-80000;}else cout<<" selamat anda mendapatkan diskon rp 70.000,00 sehingga membayar rp "<<beli-70000;}
    else if(member==3){if(beli>=1000000){cout<<"selamat anda mendapatkan diskon rp 60.000,00 sehingga membayar rp "<<beli-60000;}else cout<<" selamat anda mendapatkan diskon rp 50.000,00 sehingga membayar rp "<<beli-50000;}
    else if(member==4){if(beli>=1000000){cout<<"selamat anda mendapatkan diskon rp 40.000,00 sehingga membayar rp "<<beli-40000;}else cout<<" selamat anda mendapatkan diskon rp 30.000,00 sehingga membayar rp "<<beli-30000;}
    else if(member==5){if(beli>=1000000){cout<<"selamat anda mendapatkan diskon rp 20.000,00 sehingga membayar rp "<<beli-20000;}else cout<<" selamat anda mendapatkan diskon rp 10.000,00 sehingga membayar rp "<<beli-10000<<endl;}
        cout<<"program by :::::DARY:::::";
}}
